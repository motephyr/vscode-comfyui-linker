{
  "name": "vscode-comfyui-linker",
  "displayName": "ComfyUI Linker",
  "publisher": "markuswu",
  "description": "Generate AI images with ComfyUI workflows directly in VSCode. Enter text prompts, monitor real-time progress via WebSocket, and auto-save outputs to your workspace.",
  "version": "0.1.2",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Machine Learning"
  ],
  "keywords": [
    "comfyui",
    "stable diffusion",
    "ai",
    "image generation",
    "machine learning",
    "vscode extension"
  ],
  "activationEvents": [
    "onCommand:comfyui.generateImage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "comfyui.generateImage",
        "title": "ComfyUI: Generate Image"
      },
      {
        "command": "comfyui.generateImageFromImage",
        "title": "ComfyUI: Generate Image from Image and Prompt"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "@types/ws": "^8.18.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/motephyr/vscode-comfyui-linker.git"
  }
}
